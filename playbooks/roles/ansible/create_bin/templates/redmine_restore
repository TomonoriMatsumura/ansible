export ANSIBLE_HOST_KEY_CHECKING=False

expect -c "

    spawn ansible-playbook {{ ansible_user_dir }}/ansible/playbooks/redmine_restore.yml -S --ask-su-pass -i "{{ item.key }}:{{ ssh_port }}," -u ansible --ask-vault-pass 

    expect "SU*"

    send -- \"{{ item.value.password }}\n\"

    expect "Vault*password"

    send \"{{ vault_password }}\n\"

    expect "Enter*passphrase"

    send \"{{ vault_password }}\n\"

    interact
    "
